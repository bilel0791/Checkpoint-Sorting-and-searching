PROCEDURE insetElement(var arry ,element )
VAR
//counter
    i,n :Integer;
BEGIN
    n := arry.lenght
    i := n - 1
//condition pour determiner lemplacement exacte ou on va inserer l'element et le tableau reste triÃ©    
    WHILE ( i >= 0 and arry[i] > element) DO
         arry[i + 1] := arry[i];
        i := i - 1
    END_WHILE
// insertion d'element dans le tableau     
    arry[i + 1] := element;
// afficher le taleau avec l'element ajouter     
write(arry);
END
